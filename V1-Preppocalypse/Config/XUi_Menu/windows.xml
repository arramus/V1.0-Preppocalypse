<Preppocalypse>

	<!--remove xpath="//window[@name='newsWindow']"/-->
	<set xpath="/windows/window[@name='menuBackground']/texture/@texture">@modfolder(V1_Preppocalypse):Textures/background.png</set>	
	<set xpath="/windows/window[@name='mainMenuLogo']/texture/@texture">@modfolder(V1_Preppocalypse):Textures/7DTD_logo.png</set>
	<!--set xpath="/windows/window[@name='loadingScreen']/texture[@name='loading_image']/@texture">@modfolder(V1_Preppocalypse):Textures/loadingimage.png</set-->
	
  <!-- V1 Day Night Length default values: 10, 20, 30, 40, 50, 60, 90, 120 -->
  <set xpath="/windows/window[@name='newContinueGame']/rect[@name='tabs']/rect[@name='tabsContents']/rect[@tab_key='xuiGameSettingsGeneral']/grid/gameoption[@name='DayNightLength']/@values">10, 20, 30, 40, 50, 60, 75, 90, 120, 150, 180</set>
  <set xpath="/windows/window[@name='serverBrowser']/panel[@name='serverfilters']/panel[@controller='TabSelector']/rect[@name='tabsContents']/rect[@tab_key='xuiGameSettingsBasic']/grid/browsergameoptioncombined[@name='DayNightLength']/@values">10, 20, 30, 40, 50, 60, 75, 90, 120, 150, 180</set>

  <!-- V1 Blood Moon Enemy Count default values: 4, 6, 8, 10, 12, 16, 24, 32, 64 -->
  <set xpath="/windows/window[@name='newContinueGame']/rect[@name='tabs']/rect[@name='tabsContents']/rect[@tab_key='xuiGameSettingsBasic']/grid/gameoption[@name='BloodMoonEnemyCount']/@values">2, 3, 4, 5, 6, 7, 8, 9, 10, 12, 14, 16, 24, 32, 64, 96, 100, 125, 150</set>
  <set xpath="/windows/window[@name='serverBrowser']/panel[@name='serverfilters']/panel[@controller='TabSelector']/rect[@name='tabsContents']/rect[@tab_key='xuiGameSettingsBasic']/grid/browsergameoptioncombined[@name='BloodMoonEnemyCount']/@values">2, 3, 4, 5, 6, 7, 8, 9, 10, 12, 14, 16, 24, 32, 64, 96, 100, 125, 150</set>

  <!-- V1 Loot Respawn Days default values: -1, 5, 7, 10, 15, 20, 30, 40, 50 -->
  <set xpath="/windows/window[@name='newContinueGame']/rect[@name='tabs']/rect[@name='tabsContents']/rect[@tab_key='xuiGameSettingsAdvanced']/grid/gameoption[@name='LootRespawnDays']/@values">-1, 1, 2, 3, 4, 5, 6, 7, 10, 14, 15, 20, 30, 40, 50</set>
  <set xpath="/windows/window[@name='serverBrowser']/panel[@name='serverfilters']/panel[@controller='TabSelector']/rect[@name='tabsContents']/rect[@tab_key='xuiGameSettingsAdvanced']/grid/browsergameoptioncombined[@name='LootRespawnDays']/@values">-1, 1, 2, 3, 4, 5, 6, 7, 10, 14, 15, 20, 30, 40, 50</set>

  <!-- V1 Max Chunk Age default values: -1, 10, 20, 30, 40, 50, 75, 100 -->
  <set xpath="/windows/window[@name='newContinueGame']/rect[@name='tabs']/rect[@name='tabsContents']/rect[@tab_key='xuiGameSettingsAdvanced']/grid/gameoption[@name='MaxChunkAge']/@values">-1, 1, 2, 3, 4, 5, 6, 7, 10, 20, 30, 40, 50, 75, 100</set>
  <set xpath="/windows/window[@name='serverBrowser']/panel[@name='serverfilters']/panel[@controller='TabSelector']/rect[@name='tabsContents']/rect[@tab_key='xuiGameSettingsAdvanced']/grid/browsergameoptioncombined[@name='MaxChunkAge']/@values">-1, 1, 2, 3, 4, 5, 6, 7, 10, 20, 30, 40, 50, 75, 100</set>

  <!-- V1 Max Party Share Kill Range default values: 0, 100, 500, 1000, 5000, 10000 -->
  <set xpath="/windows/window[@name='newContinueGame']/rect[@name='tabs']/rect[@name='tabsContents']/rect[@tab_key='xuiGameSettingsMultiplayer']/grid/gameoption[@name='PartySharedKillRange']/@values">0, 10, 25, 50, 75, 100, 500, 1000, 5000, 10000</set>
  <set xpath="/windows/window[@name='serverBrowser']/panel[@name='serverfilters']/panel[@controller='TabSelector']/rect[@name='tabsContents']/rect[@tab_key='xuiGameSettingsMultiplayer']/grid/browsergameoptioncombined[@name='PartySharedKillRange']/@values">0, 10, 25, 50, 75, 100, 500, 1000, 5000, 10000</set>
	
</Preppocalypse>