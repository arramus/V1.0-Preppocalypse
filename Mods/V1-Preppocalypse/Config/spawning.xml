<configs>

	<!-- *** ZOMBIES_SCOUT_SCREAMER -->
	<set xpath="/spawning/entityspawner[@name='Scouts']/day/property[@name='TotalPerWave']/@value">1</set>
	<set xpath="/spawning/entityspawner[@name='ScoutsFeral']/day/property[@name='TotalPerWave']/@value">1</set>
	<set xpath="/spawning/entityspawner[@name='ScoutsRadiated']/day/property[@name='TotalPerWave']/@value">1</set>
	
	<!-- *** BIOME_SPAWNING -->
	<set xpath="/spawning/biome[@name='burnt_forest']/spawn[2]/@maxcount">2</set>
	<set xpath="/spawning/biome[@name='desert']/spawn[1]/@maxcount">2</set>
	<set xpath="/spawning/biome[@name='snow']/spawn[1]/@maxcount">2</set>
	<set xpath="/spawning/biome[@name='wasteland']/spawn[1]/@maxcount">2</set>

	<!-- *** PINE_FOREST_SPAWNS -->
	<append xpath="/spawning/biome[@name='pine_forest']/spawn[@entitygroup='ZombiesAll']/@notags">,oldwest,preppotown</append>
	<append xpath="/spawning/biome[@name='pine_forest']/spawn[@entitygroup='ZombiesNight']/@notags">,oldwest,preppotown</append>
	<append xpath="/spawning/biome[@name='pine_forest']">
		<spawn id="dzow01" maxcount="2" respawndelay="0.15" time="Day" entitygroup="ZombieGhostTownGroupGS1" tags="oldwest" />
		<spawn id="nzow01" maxcount="3" respawndelay="0.1" time="Night" entitygroup="ZombieGhostTownGroupGS1" tags="oldwest" />
		<spawn id="dzpreppo01" maxcount="1" respawndelay="0.3" time="Day" entitygroup="PrepperMixedEntitiesGroupGS200" tags="preppotown" /> <!-- Default is 2 -->
		<spawn id="nzpreppo01" maxcount="2" respawndelay="0.15" time="Night" entitygroup="PrepperMixedEntitiesGroupGS800" tags="preppotown" /> <!-- Default is 3 -->
	</append>

	<!-- *** BURNT_FOREST_SPAWNS -->
	<append xpath="/spawning/biome[@name='burnt_forest']/spawn[@entitygroup='ZombiesAll']/@notags">,oldwest,preppotown</append>
	<append xpath="/spawning/biome[@name='burnt_forest']/spawn[@entitygroup='ZombiesNight']/@notags">,oldwest,preppotown</append>	
	<append xpath="/spawning/biome[@name='burnt_forest']">
		<spawn id="dzow01" maxcount="3" respawndelay="0.15" time="Day" entitygroup="ZombieGhostTownGroupGS1" tags="oldwest" />
		<spawn id="nzow01" maxcount="3" respawndelay="0.1" time="Night" entitygroup="ZombieGhostTownGroupGS1" tags="oldwest" />
		<spawn id="dzpreppo01" maxcount="2" respawndelay="0.25" time="Day" entitygroup="PrepperMixedEntitiesGroupGS200" tags="preppotown" /> <!-- Default is 3 -->
		<spawn id="nzpreppo01" maxcount="3" respawndelay="0.15" time="Night" entitygroup="PrepperMixedEntitiesGroupGS800" tags="preppotown" /> <!-- Default is 4 -->
	</append>	

	<!-- *** DESERT_SPAWNS -->
	<append xpath="/spawning/biome[@name='desert']/spawn[@entitygroup='ZombiesAll']/@notags">,oldwest,preppotown</append>
	<append xpath="/spawning/biome[@name='desert']/spawn[@entitygroup='ZombiesNight']/@notags">,oldwest,preppotown</append>
	<append xpath="/spawning/biome[@name='desert']">
		<spawn id="dzow01" maxcount="3" respawndelay="0.15" time="Day" entitygroup="ZombieGhostTownGroupGS1" tags="oldwest" />
		<spawn id="nzow01" maxcount="3" respawndelay="0.1" time="Night" entitygroup="ZombieGhostTownGroupGS1" tags="oldwest" />
		<spawn id="dzpreppo01" maxcount="2" respawndelay="0.2" time="Day" entitygroup="PrepperMixedEntitiesGroupGS200" tags="preppotown" /> <!-- Default is 3 -->
		<spawn id="nzpreppo01" maxcount="2" respawndelay="0.1" time="Night" entitygroup="PrepperMixedEntitiesGroupGS800" tags="preppotown" /> <!-- Default is 3 -->
	</append>

	<!-- *** SNOW_SPAWNS -->
	<append xpath="/spawning/biome[@name='snow']/spawn[@entitygroup='SnowZombies']/@notags">,oldwest,preppotown</append>
	<append xpath="/spawning/biome[@name='snow']/spawn[@entitygroup='ZombiesNight']/@notags">,oldwest,preppotown</append>
	<append xpath="/spawning/biome[@name='snow']/spawn[@entitygroup='ZombiesAll']/@notags">,oldwest,preppotown</append>
	<append xpath="/spawning/biome[@name='snow']">
		<spawn id="dzow01" maxcount="3" respawndelay="0.15" time="Day" entitygroup="ZombieGhostTownGroupGS1" tags="oldwest" />
		<spawn id="nzow01" maxcount="3" respawndelay="0.1" time="Night" entitygroup="ZombieGhostTownGroupGS1" tags="oldwest" />
		<spawn id="dzpreppo01" maxcount="2" respawndelay="0.09" time="Day" entitygroup="PrepperMixedEntitiesGroupGS200" tags="preppotown" /> <!-- Default is 4 -->
		<spawn id="nzpreppo01" maxcount="2" respawndelay="0.06" time="Night" entitygroup="PrepperMixedEntitiesGroupGS800" tags="preppotown" /> <!-- Default is 4 -->
	</append>

	<!-- *** WASTELAND_SPAWNS -->
	<append xpath="/spawning/biome[@name='wasteland']/spawn[@entitygroup='ZombiesWasteland']/@notags">,oldwest,preppotown</append>
	<append xpath="/spawning/biome[@name='wasteland']/spawn[@entitygroup='ZombiesWastelandNight']/@notags">,oldwest,preppotown</append>	
	<append xpath="/spawning/biome[@name='wasteland']">
		<spawn id="dzow01" maxcount="3" respawndelay="0.05" time="Day" entitygroup="ZombieGhostTownGroupGS1" tags="oldwest" />
		<spawn id="nzow01" maxcount="3" respawndelay="0.025" time="Night" entitygroup="ZombieGhostTownGroupGS1" tags="oldwest" />
		<spawn id="dzpreppo01" maxcount="3" respawndelay="0.033" time="Day" entitygroup="PrepperMixedEntitiesGroupGS200" tags="preppotown" /> <!-- Default is 4 -->
		<spawn id="nzpreppo01" maxcount="3" respawndelay="0.002" time="Night" entitygroup="PrepperMixedEntitiesGroupGS800" tags="preppotown" /> <!-- Default is 4 -->
	</append>
	
</configs>